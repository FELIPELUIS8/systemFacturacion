	<queryString>
		<![CDATA[SELECT concat(cliente.nombres,' ',Cliente.apellidos) as NOMBRECLIENTE, Cliente.IDENTIFICACION, Cliente.CELULAR, Cliente.DIRRECION, Cliente.EMAIL, concat(Vendedor.nombres,' ',Vendedor.apellidos) as NOMBREVENDEDOR, detallefactura.*,
detallefactura.total
FROM Cliente, Vendedor, detallefactura

WHERE  Cliente.CODCLIENTE=$P{CODCLIENTE} AND Vendedor.CODVENDEDOR=$P{CODVENDEDOR} AND detallefactura.CODFACTURA =$P{CODFACTURA}]]>
